{
  "name": "selic",
  "version": "0.0.3",
  "description": "A tiny lib to scrap brazilian's selic, poupanÃ§a and cdi rates",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix"
  },
  "author": "Caio Ribeiro Pereira",
  "license": "MIT",
  "engines": {
    "node": ">= 10.0.0"
  },
  "repository": "https://github.com/caio-ribeiro-pereira/selic",
  "keywords": [
    "rates",
    "brazilian-rates",
    "brazilian-utils",
    "selic"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "jest": "^27.3.1",
    "nock": "^13.1.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
